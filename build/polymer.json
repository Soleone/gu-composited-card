{
  "entrypoint": "index.html",
  "fragments": [],
  "sources": [
    "src/**",
    "index.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "bundle": false,
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addPushManifest": true
    }
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}